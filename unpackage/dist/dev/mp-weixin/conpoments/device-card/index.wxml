<view class="device-card-wrapper"><view class="header"><text class="title">{{title}}</text><view></view></view><view class="main"><text class="{{['sensor-icon iconfont',sensorIcon]}}"></text><text class="sensor-name">{{sensorName}}</text><text class="sensor-status">{{sensorStatus}}</text><block wx:if="{{sensor.sensorTypeId===1}}"><u-number-box bind:input="__e" vue-id="e15884ee-1" min="{{35}}" max="{{45}}" value="{{sensorValue}}" data-event-opts="{{[['^input',[['__set_model',['','sensorValue','$event',[]]]]]]}}" bind:__l="__l"></u-number-box></block><block wx:else><block wx:if="{{sensor.sensorTypeId===2}}"><u-switch bind:input="__e" vue-id="e15884ee-2" value="{{sensorValue}}" data-event-opts="{{[['^input',[['__set_model',['','sensorValue','$event',[]]]]]]}}" bind:__l="__l"></u-switch></block></block></view><view class="footer">{{''+sensorUpdateTime+''}}</view><u-toast class="vue-ref" vue-id="e15884ee-3" data-ref="uToast" bind:__l="__l"></u-toast></view>